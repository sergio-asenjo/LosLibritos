<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
<system.serviceModel>
        <bindings>
                <basicHttpBinding>
                        <binding name="WebServiceAutorSoap">
                                <security mode="Transport" />
                        </binding>
                        <binding name="WebServiceCategoriaSoap">
                                <security mode="Transport" />
                        </binding>
                        <binding name="WebServiceClienteSoap">
                                <security mode="Transport" />
                        </binding>
                        <binding name="WebServiceEmpleadoSoap">
                                <security mode="Transport" />
                        </binding>
                        <binding name="WebServiceLibroSoap">
                                <security mode="Transport" />
                        </binding>
                        <binding name="WebServiceMultaSoap">
                                <security mode="Transport" />
                        </binding>
                        <binding name="WebServicePrestamoSoap">
                                <security mode="Transport" />
                        </binding>
                </basicHttpBinding>
        </bindings>
        <client>
                <endpoint address="https://localhost:44325/WebServiceAutor.asmx"
                        binding="basicHttpBinding" bindingConfiguration="WebServiceAutorSoap"
                        contract="ServiceAutor.WebServiceAutorSoap" name="WebServiceAutorSoap" />
                <endpoint address="https://localhost:44325/WebServiceCategoria.asmx"
                        binding="basicHttpBinding" bindingConfiguration="WebServiceCategoriaSoap"
                        contract="ServiceCategoria.WebServiceCategoriaSoap" name="WebServiceCategoriaSoap" />
                <endpoint address="https://localhost:44325/WebServiceCliente.asmx"
                        binding="basicHttpBinding" bindingConfiguration="WebServiceClienteSoap"
                        contract="ServiceCliente.WebServiceClienteSoap" name="WebServiceClienteSoap" />
                <endpoint address="https://localhost:44325/WebServiceEmpleado.asmx"
                        binding="basicHttpBinding" bindingConfiguration="WebServiceEmpleadoSoap"
                        contract="ServiceEmpleado.WebServiceEmpleadoSoap" name="WebServiceEmpleadoSoap" />
                <endpoint address="https://localhost:44325/WebServiceLibro.asmx"
                        binding="basicHttpBinding" bindingConfiguration="WebServiceLibroSoap"
                        contract="ServiceLibro.WebServiceLibroSoap" name="WebServiceLibroSoap" />
                <endpoint address="https://localhost:44325/WebServiceMulta.asmx"
                        binding="basicHttpBinding" bindingConfiguration="WebServiceMultaSoap"
                        contract="ServiceMulta.WebServiceMultaSoap" name="WebServiceMultaSoap" />
                <endpoint address="https://localhost:44325/WebServicePrestamo.asmx"
                        binding="basicHttpBinding" bindingConfiguration="WebServicePrestamoSoap"
                        contract="ServicePrestamo.WebServicePrestamoSoap" name="WebServicePrestamoSoap" />
        </client>
    </system.serviceModel>
</configuration>